{
    "name": "subjx",
    "version": "1.1.0",
    "description": "Drag, Rotate, Resize library",
    "author": "Karen Sarksyan (https://github.com/nichollascarter)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/nichollascarter/subjx.git"
    },
    "main": "index.js",
    "module": "dist/js/subjx.esm.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development rollup -c",
        "build": "cross-env NODE_ENV=production rollup -c",
        "build:all": "yarn dev && yarn build",
        "start": "cross-env NODE_ENV=development LIVE_MODE=enable rollup -c -w",
        "lint": "eslint src/ --fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "publish:local": "yarn test && yarn build:all && sh ./scripts/publish-local.sh",
        "semantic-release": "semantic-release",
        "commit": "git add . && git-cz"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.9.5",
        "@semantic-release/changelog": "^6.0.0",
        "@semantic-release/git": "^10.0.0",
        "@semantic-release/github": "^8.0.1",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "commitizen": "^4.2.4",
        "cross-env": "^5.2.1",
        "css-loader": "^1.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-loader": "^2.2.1",
        "jest": "^26.6.3",
        "jsdom": "^16.5.0",
        "mini-css-extract-plugin": "^0.4.4",
        "npm": "^6.14.6",
        "npm-cli-login": "^0.1.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "rollup": "^1.25.2",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-terser": "^5.1.3",
        "rollup-plugin-uglify": "^6.0.4",
        "semantic-release": "^18.0.0",
        "typescript": "^4.7.4",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9"
    },
    "files": [
        "LICENSE",
        "README.md",
        "index.js",
        "dist/",
        "types/*.d.ts"
    ],
    "keywords": [
        "subjx",
        "svg",
        "resize",
        "scale",
        "drag",
        "rotate",
        "vanilla-js",
        "resizable",
        "scalable",
        "draggable",
        "rotatable"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "types": "types/index.d.ts"
}
